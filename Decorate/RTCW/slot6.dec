//Was 12, now 24
Actor RTCW_VenomTracer : RTCW_Tracer
{ 
	DamageType "RTCWVenomBullet"
	Damage (0)
	States
	{
		Crash:
		Death:
			TNT1 A 0 A_Explode(30,8,!XF_HURTSOURCE,0,8)
			Goto Super::Death
		XDeath:
			TNT1 A 0 A_Explode(30,8,!XF_HURTSOURCE,0,8)
			Goto Super::XDeath
	}
}

Actor RTCW_VenomSmoke : RTCW_SmokePuff
{
	Scale 0.4
	Alpha 1.0
	States
	{
		Spawn:
			TNT1 A 0
			RW00 AAAAAAAAAA 1 A_Fadeout(0.1)
			Stop
	}
}

Actor RTCW_VenomMagazine : Ammo
{
	Inventory.MaxAmount 500
	Ammo.BackpackMaxAmount 500
	States
	{
		Spawn:
			RW00 A -1
			Stop
	}
}

