Actor UnrealGenericHeadshotGibSpawner
{
	+CLIENTSIDEONLY
	+NOINTERACTION
	+NOCLIP
	-TELESTOMP
	-SOLID
	+NOGRAVITY
	States
	{
		Spawn:
			TNT1 A 0
			TNT1 AAAAA 0 A_SpawnItemEx("Unreal_SkullPart",random(-1,1),random(-1,1),(CallACS("Unreal_GibHeightCheck",0)),random(-3,3),random(-3,3),random(1,7),Random(1,360),SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
			TNT1 A 0 A_SpawnItemEx("UnrealBloodyPuff",radius*cos(CallACS("Unreal_BloodGenerator_Return",1)),radius*sin(CallACS("Unreal_BloodGenerator_Return",1)),CallACS("Unreal_BloodGenerator_Return",2),0,0,0,0,SXF_NOCHECKPOSITION|SXF_USEBLOODCOLOR|SXF_TRANSFERTRANSLATION|SXF_ABSOLUTEPOSITION)
			TNT1 A 0 A_SpawnItemEx("UnrealBloodSprayGenerator",radius*cos(CallACS("Unreal_BloodGenerator_Return",1)),radius*sin(CallACS("Unreal_BloodGenerator_Return",1)),CallACS("Unreal_BloodGenerator_Return",2),0,0,0,0,SXF_NOCHECKPOSITION|SXF_USEBLOODCOLOR|SXF_TRANSFERTRANSLATION|SXF_ABSOLUTEPOSITION)
			Stop
	}
}

Actor Unreal_HeadshotGibGenerator : CustomInventory
{
	-COUNTITEM
	+INVENTORY.ALWAYSPICKUP
	Inventory.PickupMessage ""
	Inventory.PickupSound ""
	States
	{
		Pickup:
			TNT1 A 0
			TNT1 A 0 A_CheckFlag(NOBLOOD, "PickupFail")
			TNT1 A 0 A_SpawnItemEx("Switchable_FlyingBlood",0,0,0,random(-350,350)/100.00,random(-350,350)/100.00,random(500,1000)/100.00,random(-180,180),33)	
			TNT1 A 0 A_SpawnItemEx("UnrealGenericHeadshotGibSpawner",(radius*cos(CallACS("Unreal_BloodGenerator_Return",1))),radius*sin(CallACS("Unreal_BloodGenerator_Return",1)),CallACS("Unreal_BloodGenerator_Return",2),0,0,-0.33,0,SXF_NOCHECKPOSITION|SXF_USEBLOODCOLOR|SXF_ABSOLUTEPOSITION)
			Stop
		PickupFail:
			TNT1 A 1 A_SpawnItemEx("UnrealSmoke",radius*cos(CallACS("Unreal_BloodGenerator_Return",1)),radius*sin(CallACS("Unreal_BloodGenerator_Return",1)),CallACS("Unreal_BloodGenerator_Return",2),0,0,0,0,SXF_NOCHECKPOSITION|SXF_ABSOLUTEPOSITION)
			Stop
	}
}
		
Actor Unreal_BloodSpot 
{
	Radius 12
    Height 2
    Mass 1
	RenderStyle Translucent
	Alpha 0.9
	+NOINTERACTION
	+FLOORHUGGER
	-NOGRAVITY
	+THRUACTORS
	-SOLID
	States
	{
		Spawn:
			TNT1 A 0 
			TNT1 A 0 A_CheckFloor(1)
			stop
			UN00 A 2100
			TNT1 A 0 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraDecorate",31) == 1, "Spawn")
			UN00 AAAAAAAAAA 1 A_Fadeout(0.1)
			stop
	}
}		