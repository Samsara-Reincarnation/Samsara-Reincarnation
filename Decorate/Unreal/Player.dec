Actor UnrealClass : Boolean { }

Actor UnrealPlayer : PlayerPawn 
{
	Player.DisplayName "Prisoner849"
	Player.StartItem "Clip", 50
	Player.StartItem "DPistolAmmo", 50
	Player.StartItem "Unreal_AutoMag", 1
	Player.StartItem "Unreal_DispersionPistol", 1
	Player.StartItem "UnrealClass", 1
    Player.StartItem "IsSamsaraClass", 1
	Player.StartItem "TauntButton", 1	
	Player.Soundclass "Prisoner849"
	Player.JumpZ 10//Default is 8
	Species "Player"
	Scale 0.6
	Player.ViewHeight 48
	BloodType "UnrealBloodyPuff"
	DamageFactor "RebelMissile", 0
    DamageFactor "PoisonCloud", 1.75
    //DamageFactor "Wraithverge", 0.75
	DamageFactor "DynamiteCoop", 0.0	
    DamageFactor "CorvusCrossbow", 1.1
    DamageFactor "MightyBoot", 1.5
    DamageFactor "DragonClaw", 1.1
    DamageFactor "WMCBeam", 0.75
    DamageFactor "WMCNade", 0.75
	DamageFactor "EnemyInfightDamage", 0.0
	DamageFactor "ElectricBolt", 1.0
	DamageFactor "PhosFireCoop", 0.0
	DamageFactor "EradCoopExplosive2", 0.0
    DamageFactor "C7MineBoom", 0.0	
    DamageFactor "EradCoopGreenFire", 0.0	
	DamageFactor "BCannonProj", 0.75	
	BloodType "SamsaraBloodyHellBlood"
    DamageFactor "RMR-BigPlasma", 0.85		
	DamageFactor "SWNukeCoop", 0.0		
	DamageFactor "SWRailgunCoop", 0.0
	DamageFactor "SWFlameCoop", 0.0
	DamageFactor "SWFlame2Coop", 0.0
	DamageFactor "SWStickyCoop", 0.0	
	DamageFactor "CMDMGSHIT", 0.0	
    +NOSKIN
    -DONTBLAST
    -THRUSPECIES
    Species "Player"
    Player.ColorRange 112, 127	
	var int user_scalex;
	var int user_scaley;	
	States
	{
		Spawn:
			UN00 A 0 NoDelay A_JumpIf((momx > 0.1 || momx < -0.1) || (momy > 0.1 || momy < -0.1),"See")
			UN00 A 6
			UN00 B 6
			UN00 C 6
			UN00 D 6
			UN00 E 6
			UN00 F 6
			UN00 G 6
			Loop
		See:
			UN01 A 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Spawn")
			UN01 B 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Spawn")
			UN01 C 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Spawn")
			UN01 D 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Spawn")
			UN01 E 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Spawn")
			UN01 F 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Spawn")
			UN01 G 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Spawn")
			UN01 H 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Spawn")
			UN01 I 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Spawn")
			Loop
		Melee:	
		Missile:
			UN02 A 2 A_JumpIf((momx > 0.1 || momx < -0.1) || (momy > 0.1 || momy < -0.1),"RunFire")
			UN02 B 2 A_JumpIf((momx > 0.1 || momx < -0.1) || (momy > 0.1 || momy < -0.1),"RunFire")
			UN02 C 2 A_JumpIf((momx > 0.1 || momx < -0.1) || (momy > 0.1 || momy < -0.1),"RunFire")
			UN02 D 2 A_JumpIf((momx > 0.1 || momx < -0.1) || (momy > 0.1 || momy < -0.1),"RunFire")
			UN02 E 2 A_JumpIf((momx > 0.1 || momx < -0.1) || (momy > 0.1 || momy < -0.1),"RunFire")
			UN02 F 2 A_JumpIf((momx > 0.1 || momx < -0.1) || (momy > 0.1 || momy < -0.1),"RunFire")
			Goto Spawn
		RunFire:
			UN01 J 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			UN01 K 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			UN01 L 2  A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			UN01 M 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			UN01 N 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			UN01 O 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			UN01 P 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			UN01 Q 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			UN01 R 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			UN01 S 2 A_JumpIf((momx < 0.1 && momx > -0.1) && (momy < 0.1 && momy > -0.1),"Missile")
			Goto Spawn
		Pain:
			UN03 A 0 A_Jump(256, "LeftPain", "RightPain", "GutPain", "HeadPain")
			Goto LeftPain
		LeftPain:
			UN03 A 6 A_Pain
			Goto Spawn
		RightPain:
			UN03 C 6 A_Pain
			Goto Spawn	
		GutPain:
			UN03 B 6 A_Pain
			Goto Spawn	
		HeadPain:
			UN03 D 6 A_Pain
			Goto Spawn	
		Death:
			UN04 A 2 A_PlayerScream
			UN04 B 2 A_NoBlocking
			UN04 C 2
			UN04 D 2
			UN04 E 2
			UN04 F 2
			UN04 G 2
			UN04 H 2
			UN04 I 2
			UN04 J 2
			UN04 K 2
			UN04 L 2
			UN04 M 2
			UN04 N 2
			UN04 O 2
			UN04 P 2
			UN04 Q 2
			UN04 R 2
			UN04 S 2
			UN04 T 2
			UN04 U 2
			UN04 V -1
			Stop
		XDeath:
			TNT1 A 0 A_PlaySound("Unreal/Enemygibbed", CHAN_BODY)
			TNT1 A 0 A_SpawnItemEx("Unreal_GibF2Head",0,0,43,random(-2,4),random(-2,4),random(2,4),0,SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
			TNT1 AA 0 A_SpawnItemEx("Unreal_GibThigh",random(-1,1),random(-1,1),random(28,35),random(-3,3),random(-3,3),random(0,7),Random(1,360),SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
			TNT1 A 0 A_SpawnItemEx("Unreal_GibHeart",random(-1,1),random(-1,1),random(28,35),random(-3,3),random(-3,3),random(0,7),Random(1,360),SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
			TNT1 AA 0 A_SpawnItemEx("Unreal_GibFlesh1",random(-1,1),random(-1,1),random(5,22),random(-3,3),random(-3,3),random(0,7),Random(1,360),SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
			TNT1 AA 0 A_SpawnItemEx("Unreal_GibFlesh2",random(-1,1),random(-1,1),random(30,38),random(-3,3),random(-3,3),random(0,7),Random(1,360),SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
			TNT1 A 0 A_SpawnItemEx("Unreal_GibStomach",random(-1,1),random(-1,1),random(25,40),random(-3,3),random(-3,3),random(0,7),Random(1,360),SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
			TNT1 AA 0 A_SpawnItemEx("Unreal_GibArm",random(-1,1),random(-1,1),random(8,40),random(-3,3),random(-3,3),random(0,7),Random(1,360),SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
			TNT1 A 0 A_SpawnItemEx("Unreal_GibLiver",random(-1,1),random(-1,1),random(8,40),random(-3,3),random(-3,3),random(0,7),Random(1,360),SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
			TNT1 A 0 A_SpawnItemEx("Unreal_BloodSpot",0,0,0,0,0,0,0,SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)		
			TNT1 A 2 A_NoBlocking
			TNT1 A -1 A_FadeOut
			Stop
		Disintegrate:
			"####" "#" 0 A_UnsetSolid
			"####" "#" 0 ACS_NamedExecuteAlways("ExtraDeathFX",0,1)
			"####" "#" 0 A_PlayerScream
			"####" "#" 0 A_Stop
			"####" "#" 3 A_SpawnItem("DisintEffectSmall",0,10)
			"####" "#" 3 A_SetTranslucent(0.9,0)
			"####" "#" 0 ACS_NamedExecuteAlways("ExtraDeathFX",0,1)
			"####" "#" 3 A_SetTranslucent(0.8,0)
			"####" "#" 0 ACS_NamedExecuteAlways("ExtraDeathFX",0,1)
			"####" "#" 3 A_SetTranslucent(0.7,0)
			"####" "#" 0 ACS_NamedExecuteAlways("ExtraDeathFX",0,1)
			"####" "#" 3 A_SetTranslucent(0.6,0)
			"####" "#" 0 ACS_NamedExecuteAlways("ExtraDeathFX",0,1)
			"####" "#" 3 A_SetTranslucent(0.5,0)
			"####" "#" 0 ACS_NamedExecuteAlways("ExtraDeathFX",0,1)
			"####" "#" 2 A_SetTranslucent(0.4,0)
			"####" "#" 0 ACS_NamedExecuteAlways("ExtraDeathFX",0,1)
			"####" "#" 2 A_SetTranslucent(0.3,0)
			"####" "#" 0 ACS_NamedExecuteAlways("ExtraDeathFX",0,1)
			"####" "#" 2 A_SetTranslucent(0.2,0)
			"####" "#" 0 ACS_NamedExecuteAlways("ExtraDeathFX",0,1)
			"####" "#" 2 A_SetTranslucent(0.1,0)
			TNT1 A -1
			Stop			
	}		
}	