actor Clip2: CustomInventory
{
    Inventory.PickupMessage "Picked up 5 rounds of Ammo 1."
    Inventory.PickupSound "ammo/bulletsmall"
    States
    {
      Spawn:
        CLIP B -1
        stop	
      Pickup:
        TNT1 A 0 A_JumpIfInventory("MarathonClass",1,"PickupMarathon")
        TNT1 A 0 A_JumpIfInventory("RottClass",1,"PickupRott")
		TNT1 A 0 A_JumpIfInventory("CatacombClass", 0, "PickupCatacomb")
        TNT1 A 0 A_JumpIfInventory("Clip",0,"PickupFail")
        TNT1 A 0 A_GiveInventory("Clip",5)
        stop
	  PickupRott:
		TNT1 A 0 A_JumpIfInventory("HSMissiles", 0, "PickupFail")
	    TNT1 A 0 A_GiveInventory("HSMissiles", 1)
		Stop
	  PickupCatacomb:
		TNT1 A 0 A_GiveInventory("Catacomb_Zappers", Random(0,1))
		TNT1 A 0 A_GiveInventory("Catacomb_Waves", Random(0,1))
        stop
      PickupMarathon:
        TNT1 A 0 A_JumpIfInventory("Clip",0,"PickupFail")
        TNT1 A 0 A_SetBlend("Green",0.4,4)
        TNT1 A 1 A_GiveInventory("Clip",5)
        stop
      PickupFail:
        CLIP B 1
        fail
    }
}

