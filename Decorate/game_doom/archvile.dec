	Game Doom
	SpawnID 111
	Health 700
	Radius 20
	Height 56
	Mass 500
	Speed 15
	PainChance 10
	Monster
	MaxTargetRange 896
	+QUICKTORETALIATE 
	+FLOORCLIP 
	+NOTARGET
    +FIREDAMAGE
    +DONTRIP
    +NOTDMATCH
    +NOICEDEATH
	SeeSound "vile/sight"
	PainSound "vile/pain"
	DeathSound "vile/death"
	ActiveSound "vile/active"
	MeleeSound "vile/stop"
	Obituary "$OB_VILE"
	Tag "Arch-vile"
	var int user_scalex;
	var int user_scaley;
	Species "Monster"
