    Inventory.Icon "IBOMZ0"
    Inventory.PickupSound "greed/pickup"
    Inventory.PickupMessage "You picked up the Time Bomb!"
	Inventory.RestrictedTo "POGreedPlayer"	
	Tag "Time Bomb"
    States
    {
      Spawn:
        IBOM X 1 Bright
        Loop
      Use:
		TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"UseCoop")	
