actor IPOGTimeBombItem : CustomInventory
{
    +FLOATBOB
    Inventory.Amount 1
    Inventory.MaxAmount 10
    +INVBAR
	+INVENTORY.HUBPOWER
    +INVENTORY.UNDROPPABLE
