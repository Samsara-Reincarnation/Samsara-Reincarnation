        TNT1 A 0 A_FireCustomMissile("IPOGTimeBomb",0,0,0,-40,0,0)
        Stop
	  UseCoop:
		TNT1 A 0 A_FireCustomMissile("IPOGTimeBombCoop",0,0,0,-40,0,0)
        Stop
      UseFail:
        TNT1 A 0
        fail
    }
}

