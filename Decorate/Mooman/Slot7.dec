Actor PGSuperPlasmaTrail
{
	PROJECTILE
	Speed 0
	Damage (0)
	+CLIENTSIDEONLY
	+NOINTERACTION
	+THRUGHOST
	+THRUSPECIES
	Species "Player"
	//renderstyle add
	scale 1.1
	states
	{
	Spawn:
		NULL A 2 BRIGHT
	SpawnLoop:
		GP70 A 1 bright A_Fadeout(0.1)
		loop
	}
}

Actor PGSuperPlasmaPuff
{
