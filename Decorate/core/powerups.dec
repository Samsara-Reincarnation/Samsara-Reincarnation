actor PartialInvisibility : CustomInventory replaces BlurSphere
{
    RenderStyle Add
    Alpha 0.75
    +Floatbob
    +VISIBILITYPULSE
    +INVENTORY.PICKUPFLASH
    +INVENTORY.BIGPOWERUP
    +COUNTITEM
	+INVENTORY.QUIET
    Inventory.PickupAnnouncerEntry "partialinvisibility"
    Inventory.PickupMessage "$SAMSARA_POWERUP_INVIS"
    Inventory.PickupSound "nosoundforyoubitch"
    states
    {
      Spawn:
        NVSI A 0 NoDelay ACS_NamedExecuteWithResult("Samsara_SwitchableObject",2)
        NVSI A -1 BRIGHT ACS_NamedExecuteAlways("SamsaraClientDecorate", 0, 4, 100, 75)
        stop
      Pickup:
		TNT1 A 1 A_JumpIfInventory("StrifeClass",1,"PickupStrife")
		TNT1 A 0 ACS_NamedExecute("InvisPickupMessage")
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)	  
        TNT1 A 1 A_JumpIfInventory("MarathonClass",1,"PickupMarathon")
        TNT1 A 1 A_JumpIfInventory("HexenClass",1,"PickupHexen")
        TNT1 A 1 A_JumpIfInventory("CorvusClass",1,"PickupHeretic")
        TNT1 A 1 A_JumpIfInventory("QuakeClass",1,"PickupQuake")
        TNT1 A 1 A_JumpIfInventory("Doom64GuyClass",1,"PickupDoom64")		
		TNT1 A 1 A_JumpIfInventory("KatarnClass",1,"PickupKatarn")		
		TNT1 A 1 A_JumpIfInventory("WitchavenClass",1,"PickupWitchaven")
		TNT1 A 1 A_JumpIfInventory("HalfLifeClass",1,"PickupHalfLife")
		TNT1 A 1 A_JumpIfInventory("JonClass",1,"PickupJon")	
		TNT1 A 1 A_JumpIfInventory("BittermanClass",1,"PickupBitterman")
		TNT1 A 1 A_JumpIfInventory("Hexen2Class",1,"PickupHexen2")
		TNT1 A 1 A_JumpIfInventory("BondClass",1,"PickupGoldeneye")
        TNT1 A 0 A_PlaySound("invisibilityget")
        TNT1 A 1 A_GiveInventory("BlurSphere")
        stop
	  PickupStrife:
		TNT1 A 0 ACS_NamedExecute("InvisPickupMessage")
		TNT1 A 0 A_SetBlend("d7ba45", 0.3, 4)	
        TNT1 A 0 A_JumpIfInventory("PortShadowArmor",0,"PickupHereticFail")
        TNT1 A 0 A_PlaySound("strife/itemget")
        TNT1 A 1 A_GiveInventory("PortShadowArmor")
        stop		
	  PickupDoom64:
        TNT1 A 0 A_PlaySound("invisibilityget")
        TNT1 A 1 A_GiveInventory("PowerTranslucency")
        stop	  
      PickupKatarn:
		TNT1 A 0 A_PlaySound("invisibilityget")
		TNT1 A 1 A_GiveInventory("PowerTranslucency")
		stop
	  PickupWitchaven:
		TNT1 A 0 A_PlaySound("gpickup/invis")
		TNT1 A 1 A_GiveInventory("BlurSphere")
		stop	
	  PickupJon:
		TNT1 A 0 A_PlaySound("Jon/FancyItemPick")
		TNT1 A 1 A_GiveInventory("EyeOfSiaItem_EX")
		stop
	  PickupBitterman:
		TNT1 A 0 A_PlaySound("Q2Misc/Pickup")
		TNT1 A 0 A_Jump(128, "PickupQ2_2")
		TNT1 A 1 A_GiveInventory("Q2Silencer")
		stop
	  PickupQ2_2:
		TNT1 A 1 A_GiveInventory("Q2DualFireDamage")
		stop	  
      PickupHeretic:
        TNT1 A 0 A_JumpIfInventory("PortGhostly",0,"PickupHereticFail")
        TNT1 A 0 A_PlaySound("heretic/itemget")
        TNT1 A 1 A_GiveInventory("PortGhostly")
        stop
      PickupHereticFail:
        NVSI A 1 BRIGHT
        fail
      PickupHexen:
        TNT1 A 0 A_PlaySound("invisibilityget")
        TNT1 A 1 A_GiveInventory("PowerGhost")
        stop
      PickupHexen2:
        TNT1 A 0 A_JumpIfInventory("PortGhostly2",0,"PickupHereticFail")
        TNT1 A 0 A_PlaySound("hexen2/artifactpickup")
        TNT1 A 1 A_GiveInventory("PortGhostly2")
        stop	
	  PickupDuke:
        TNT1 A 0 A_PlaySound("duke/itemget")
        TNT1 A 1 A_GiveInventory("ItemHoloDuke",3)
        stop
      PickupQuake: 
        TNT1 A 0 A_GiveInventory("QuakeInvisTimer", 1050)
        TNT1 A 0 ACS_NamedExecuteAlways("SamsaraDecorate",0,25,0,0)//TNT1 A 0 A_PlaySoundEx("quake/invisannouncer","soundslot7",0,2)
        TNT1 A 0
        stop
      PickupMarathon:
        TNT1 A 0 A_SetBlend("Green",0.5,6)
        TNT1 A 0 A_PlaySound("marathon/bigitemget")
        //TNT1 A 1 A_GiveInventory("Blursphere")
        TNT1 A 0 ACS_NamedExecuteAlways("SamsaraDecorate",0,26,0,0) //A_GiveInventory("PowerShadow")
        stop
	  PickupHalfLife:
        TNT1 A 0 A_PlaySound("invisibilityget")
        TNT1 A 1 A_GiveInventory("PowerTranslucency")
        stop
	  PickupHexen2:
        TNT1 A 0 A_PlaySound("invisibilityget")
        TNT1 A 1 A_GiveInventory("PowerGhost")
        stop
	  PickupGoldeneye:
        TNT1 A 0 A_PlaySound("invisibilityget")
        TNT1 A 1 A_GiveInventory("PowerTranslucency")
        stop
    }
}

