			GE00 A 1 A_SetPitch(pitch - 5, SPF_INTERPOLATE)
			Loop
		XDeathFloor:
			TNT1 A 0 A_NoGravity
			TNT1 A 0 A_Playsound("Goldeneye/Mineplant", CHAN_5)
			TNT1 A 0 A_CustomMissile("Goldeneye_RemoteMineStatic", 0, 0, angle, CMF_TRACKOWNER)
			stop
	}
}

