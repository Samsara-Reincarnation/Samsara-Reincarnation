actor SamsaraFlashLightAmmoDisplay : Counter {}

Actor HLFlashLight
{
	+NOGRAVITY
	+DONTSPLASH
	+PUFFONACTORS
	+BLOODLESSIMPACT
	+ALWAYSPUFF
	+CLIENTSIDEONLY
	+NOTRIGGER
	Radius 2
	Height 2
	States
	{
		Spawn:
			TNT1 A 0
			TNT1 A 0 A_JumpIf(CallACS("Samsara_GZDoomChecker") == true, "Spawn2")
			HL55 A 1
			stop	
		Spawn2:
			HL55 A 3
			stop
	}
}

Actor HLStep 
{
	+NOBLOCKMAP
	+CLIENTSIDEONLY
	+NOGRAVITY
	+NOINTERACTION
	Height 4
	Radius 2
	States
	{
		Spawn:
			TNT1 A 0
			TNT1 A 0 A_CheckFloor("Step")
			TNT1 A 0 A_JumpIf(waterlevel >= 1, "Wade")
			TNT1 A 1
			stop
		Step:
			TNT1 A 0 A_Playsound("HLPlayer/Step", CHAN_BODY, 0.8)
			TNT1 A 1
			stop
		Wade:
			TNT1 A 0 A_JumpIf(waterlevel > 2, "Swim")
			TNT1 A 0 A_Playsound("HLPlayer/Wade", CHAN_BODY, 0.8)
			TNT1 A 1
			stop
		Swim:
			TNT1 A 0 A_Playsound("HLPlayer/Swim", CHAN_BODY, 0.8)
			TNT1 A 1
			stop
	}
}

actor HLPCVNightVision : PowerLightAmp
