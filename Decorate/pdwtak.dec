actor PeopleDieWhenTheyAreKilled
{
    +NOINTERACTION
    +FORCERADIUSDMG

    Projectile
    Speed 0

    Radius 8
    Height 8

    DamageType "Kamikaze"
    Obituary "$SAMSARA_OB_PDWTAK"

    States
    {
      Spawn:
      Death:
        TNT1 A 0
        TNT1 A 0 A_GiveToTarget("KamikazeImmunity")
        TNT1 A 280 A_Explode(512, 384, 0)
        TNT1 A 0 A_TakeFromTarget("KamikazeImmunity")
        stop
    }
}

