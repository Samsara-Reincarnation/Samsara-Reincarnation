		Goto PickupDisrupSuccess		
		TNT1 A 0 A_JumpIfInventory("DisruptorHiFreq", 0, "PickupFail")
		Goto PickupDisrupSuccess
	  PickupDisrupSuccess:
        TNT1 A 0 A_GiveInventory("RocketAmmo",5)
        TNT1 A 0 A_GiveInventory("Cell",10)
		TNT1 A 0 A_GiveInventory("DisruptorHiFreq",10)
		TNT1 A 0 A_GiveInventory("DisruptorBinaryLockOn", 5)
		Stop
		
      PickupCatacomb:
		TNT1 A 0 A_JumpIfInventory("Catacomb_Nukes", 0, "PickupCatacomb2")
	    TNT1 A 0 A_GiveInventory("Catacomb_Nukes", Random(3,6))
		TNT1 A 0 A_GiveInventory("Catacomb_Bolts", Random(1,3))
	    Stop
	 PickupCatacomb2:
	    TNT1 A 0 A_JumpIfInventory("Catacomb_Bolts", 0, "PickupFail")
		TNT1 A 0 A_GiveInventory("Catacomb_Bolts", Random(1,3))
		Stop			

      PickupFail:
        AMSM F 1
        fail		
    }
