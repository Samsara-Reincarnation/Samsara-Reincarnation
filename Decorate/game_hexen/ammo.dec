		TNT1 A 0 A_GiveInventory("MultiRocketAmmo",2)
		TNT1 A 0 A_Jump(66,1)
        stop
		TNT1 A 0 A_GiveInventory("PlasmaCell",1)
		Stop
		
	  CheckStrife:
		TNT1 A 0 A_JumpIfInventory("Cell", 0, 1)
		Goto PickupStrSuccess
		TNT1 A 0 A_JumpIfInventory("RocketAmmo", 0, 1)
		Goto PickupStrSuccess
