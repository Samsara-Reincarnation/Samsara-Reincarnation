      Spawn:
        AMSM F 20 Light("SMAMMO3POINT")
        AMSM F 20 Light("SMAMMO4POINT") BRIGHT
        loop
      Pickup:
	    TNT1 A 0 A_JumpIfInventory("DukeClass", 0, "PickupDuke")
