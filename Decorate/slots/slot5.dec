actor SamsaraSlotFiveDrop: SamsaraSlotFivePickup
{
  States
  {
    Spawn:
      TNT1 A 0
      goto SpawnLoop
    Pickup:
      TNT1 A 0 A_JumpIfInventory("HalfLifeClass", 1, "CheckHL")
	  TNT1 A 0 A_JumpIfInventory("StrifeClass", 1, "CheckStrife")
	  TNT1 A 0 A_JumpIfInventory("MarathonClass", 0, "CheckMarathon")
	  TNT1 A 0 A_JumpIfInventory("BondClass", 1, "CheckBond")
      TNT1 A 0 A_JumpIf((ACS_NamedExecuteWithResult("SamsaraGiveWeapon", 5, 1) & 1) == 1, "PickupSuccess")
      Goto PickupFailure
	CheckStrife:
	  TNT1 A 0 A_JumpIfInventory("GotWeapon5", 1, "StrifeCheck")
	  TNT1 A 0 A_GiveInventory("StrifeSigilPiece",1)
	  TNT1 A 0 A_GiveInventory(" Sigil ", 1)
	StrifeCheck:	
      TNT1 A 0 A_JumpIf((ACS_NamedExecuteWithResult("SamsaraGiveWeapon", 5) & 3) == 1, "PickupSuccess")
      goto PickupFailure
	CheckHL:
	  TNT1 A 0 A_JumpIfInventory(" RPG ", 1, "HLPickupAmmo")
      TNT1 A 0 A_JumpIf((ACS_NamedExecuteWithResult("SamsaraGiveWeapon", 5, 1) & 1) == 1, 1)
      Goto PickupFailure	
	  TNT1 A 0 ACS_NamedExecuteAlways("HL2685",0,15)
	  Goto PickupSuccess
	HLPickupAmmo:
	  TNT1 A 0 A_JumpIfInventory("RocketAmmo",0,"PickupFailure")
	  TNT1 A 0 A_GiveInventory("RocketAmmo",3)
	  TNT1 A 0 ACS_NamedExecuteAlways("SamsaraClientWeaponPickup",0, 6)
	  Stop
	CheckMarathon:
	  TNT1 A 0 A_JumpIfInventory("SPNKR-XP SSM Launcher", 1, "MarathonPickupAmmo")
	  TNT1 A 0 A_JumpIf((ACS_NamedExecuteWithResult("SamsaraGiveWeapon", 5) & 3) == 1, "PickupSuccess")
	  Goto PickupFailure
	MarathonPickupAmmo:
	  TNT1 A 0 A_JumpIfInventory("RocketAmmo", 0, "PickupFailure")
	  TNT1 A 0 A_SetBlend("Green", 0.3, 3)
	  TNT1 A 0 ACS_NamedExecuteAlways("SamsaraClientWeaponPickup",0, 6)
	  TNT1 A 0 A_GiveInventory("RocketAmmo", 8)
	  Stop	  
  }
}

actor SamsaraGLReplace : SamsaraSlotFivePickup replaces GrenadeLauncher 
{
	States
	{
		IWasDropped:
			Goto SpawnLoop	
		Doomguy:  GLAU A 35 A_ChangeFlag("FLOATBOB", 0)   goto SpawnLoop
		Pickup:
			TNT1 A 0 A_JumpIfInventory("DoomGuyClass", 1, "CheckDoomGuy")	 
			TNT1 A 0 A_JumpIfInventory("HalfLifeClass", 1, "CheckHL")
			TNT1 A 0 A_JumpIfInventory("StrifeClass", 1, "CheckStrife")	 
			TNT1 A 0 A_JumpIfInventory("MarathonClass", 0, "CheckMarathon")
			TNT1 A 0 A_JumpIfInventory("BondClass", 1, "CheckBond")	
			TNT1 A 0 A_JumpIf((ACS_NamedExecuteWithResult("SamsaraGiveWeapon", 5) & 3) == 1, "PickupSuccess")
			Goto PickupFailure
		CheckDoomGuy:
			TNT1 A 0 A_JumpIfInventory("Doom64Mode", 1, 2)	
			TNT1 A 0 A_JumpIf((ACS_NamedExecuteWithResult("SamsaraGiveWeapon", 9) & 3) == 1, "PickupSuccess")
			Goto PickupFailure
			TNT1 A 0 A_JumpIf((ACS_NamedExecuteWithResult("SamsaraGiveWeapon", 5) & 3) == 1, "PickupSuccess")
			Goto PickupFailure
	}
}
