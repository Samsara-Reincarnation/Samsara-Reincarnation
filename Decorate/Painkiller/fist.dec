Actor Painkiller_Painkiller_Blades_Return
{
	+NOGRAVITY
	+THRUACTORS
	+NOINTERACTION
	-SOLID
	+DROPOFF
	+NOCLIP
	Height 16
	Radius 8
	States
	{
		Spawn:
			TNT1 A 0
			PK00 A 0 ACS_NamedExecuteAlways("Painkiller_Painkiller_WarpBack")
		SpawnLoop:
			PK00 A 2 A_JumpIfInventory("Painkiller_PainkillerReturned",1,"Remove",AAPTR_TARGET)
			Loop
		Remove:
			TNT1 A 2
			Stop
	}
}

