Actor Painkiller_Painkiller_BeamFire
{
	-SOLID
	+THRUACTORS
	-SHOOTABLE
	+NOINTERACTION
	+DONTHARMSPECIES
	+DONTHURTSPECIES
	Species "Player"
	Height 4
	Radius 2
	States
	{
		Spawn:
			TNT1 A 0
			TNT1 A 0 A_TransferPointer(AAPTR_TARGET,AAPTR_DEFAULT,AAPTR_TRACER,AAPTR_MASTER)
			TNT1 A 0 A_ReArrangePointers(AAPTR_MASTER,AAPTR_TARGET,AAPTR_NULL)
			//TNT1 A 0 A_JumpIf(CallACS("Hexen2_PrintPointer",2)==1,1)
		SpawnLoop:
			PK00 A 0 A_JumpIfInventory("Painkiller_PainkillerAlt",1,1,AAPTR_MASTER)
			Goto Remove
			TNT1 A 0 A_Warp(AAPTR_MASTER,0,8,36,0,WARPF_NOCHECKPOSITION|WARPF_COPYINTERPOLATION,1)
			TNT1 A 0 A_JumpIfTargetInLOS(2,20)
			TNT1 A 0 A_StopSound(CHAN_5)
			Goto SpawnLoop+7
			TNT1 A 0 A_PlaySound("Painkiller/Painkiller/AltLoop",CHAN_5,1.0,true)
			TNT1 A 0 A_CustomMissile("Painkiller_Painkiller_Laser",0)
			TNT1 A 0 A_CustomMissile("Painkiller_Painkiller_Laser_Damage",0)
			TNT1 A 2
			Loop
		Remove:
			TNT1 A 0 A_StopSound(CHAN_5)
			TNT1 A 2
			Stop
	}
}

