Actor ShadowCaster_Item : Trigger
{
	Inventory.MaxAmount 0
	+AUTOACTIVATE
	States
	{
		Spawn:
			SC99 A -1
			Stop
		Pickup:
			TNT1 A 0 A_JumpIf(CallACS("ShadowCaster_AddToBag",0)==0,"Failure")
			Goto PickupStop
		Failure:
			TNT1 A 0
			fail
	}
}

