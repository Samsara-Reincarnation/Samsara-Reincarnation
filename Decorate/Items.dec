Actor ShadowCaster_ShurikenProjectile
{
	Projectile
	Speed 20
	Damage (random(3,6)*10)
	Radius 8
	Height 16
	States
	{
		Spawn:
			SC00 ABCDABCD 3
			SC00 A 0 A_ScaleVelocity(-1)
			SC00 ABCDABCD 3
			TNT1 A 2 ACS_NamedExecuteWithResult("ShadowCaster_ShurikenRedeem",0,1)
			Stop
		Death:
			SC00 E 0 A_PlaySound("ShadowCaster/ShurikenHit")
			SC00 E 7 //This prevents the shuriken from being spammed, while keeping a 2 tic delay after the script is run as a precaution and because I can
			SC00 E 2 ACS_NamedExecuteWithResult("ShadowCaster_ShurikenRedeem",0,1)
			Stop
	}
}

