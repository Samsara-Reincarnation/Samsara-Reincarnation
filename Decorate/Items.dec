Actor ShadowCaster_InsectAppearance : Counter { Inventory.MaxAmount 3 }

Actor ShadowCaster_InsectProjectile
{
	Projectile
	Speed 10
	Damage (random(1,2)*10)
	Radius 8
	Height 16
	States
	{
		Spawn:
			TNT1 A 0
			TNT1 A 0 A_GiveInventory("ShadowCaster_InsectAppearance",random(1,3))
			TNT1 A 0 A_JumpIfInventory("ShadowCaster_InsectAppearance",3,"Spawn3")
			TNT1 A 0 A_JumpIfInventory("ShadowCaster_InsectAppearance",2,"Spawn2")
			TNT1 A 0 A_JumpIfInventory("ShadowCaster_InsectAppearance",1,"Spawn1")
		Spawn1:
			SC06 ABC 3
			Loop
		Spawn2:
			SC06 DEF 3
			Loop
		Spawn3:
			SC06 GHI 3
			Loop
		Death:
			TNT1 A 0 A_JumpIfInventory("ShadowCaster_InsectAppearance",3,"Death3")
			TNT1 A 0 A_JumpIfInventory("ShadowCaster_InsectAppearance",2,"Death2")
			TNT1 A 0 A_JumpIfInventory("ShadowCaster_InsectAppearance",1,"Death1")
		Death1:
			SC06 ABCABC 3
			Stop
		Death2:
			SC06 DEFDEF 3
			Stop
		Death3:
			SC06 GHIGHI 3
			Stop
	}
}

