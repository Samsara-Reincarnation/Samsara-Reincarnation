Actor DeusExBullet : FastProjectile
{
	Speed 150
	Damage (0)
	Radius 1
	Height 2
	+BLOODSPLATTER
	+NOTIMEFREEZE
	+NOEXTREMEDEATH
	+CANNOTPUSH
	+HITTRACER
	+FORCERADIUSDMG
	+NODAMAGETHRUST
	DamageType "DeusExShot"
	RenderStyle Shaded
	StencilColor "af af 61"
	Decal "DeusEx_BulletChip"
	Var Int user_damage;
	states
	{
	Spawn:
		TNT1 A 0 NODELAY //A_GiveInventory("DeusEx_PointBlank", 1)
		TNT1 A 1
		Goto Spawn2
	Spawn2:
		DX00 A 1 Bright //A_TakeInventory("DeusEx_PointBlank", 99999)
	SpawnLoop:
		DX00 A 1 Bright
		loop
	Death:
		//TNT1 A 0 A_PlaySound("DeusEx/Bullet/HitStone", CHAN_BODY)
		TNT1 A 0 A_SpawnItemEx("DeusEx_RockChip", Random(-1,1), -2, 0, Random(-1,1), Random(-6,-2), Random(5,1), angle-180, SXF_ABSOLUTEVELOCITY)
		//TNT1 A 0 A_CustomMissile("DeusEx_RockChip", 0, 0, Angle-180, CMF_TRACKOWNER)
		TNT1 A 1 A_SpawnItemEx("DeusEx_BulletPuff",0,0,0,0,0,1,0,0)
		stop
	Crash:		
		TNT1 A 0 A_PlaySound("DeusEx/Bullet/HitRobot", CHAN_BODY)
		TNT1 A 1 A_Explode(CallACS("DeusEx_DamageModifiers",2),8,!XF_HURTSOURCE,0,8)
		stop	
	XDeath:
		//TNT1 A 0 A_PlaySound("DeusEx/Bullet/HitFlesh", CHAN_BODY)
		TNT1 A 1 A_Explode(CallACS("DeusEx_DamageModifiers",2),8,!XF_HURTSOURCE,0,8)
		stop
	}
}

