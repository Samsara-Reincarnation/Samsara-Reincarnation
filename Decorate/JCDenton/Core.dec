Actor DeusExGore 
{
	PROJECTILE
	-NOGRAVITY
	+NOBLOCKMAP
	+MOVEWITHSECTOR
	+NOTELEPORT
	-SOLID
	-TELESTOMP
	+CLIENTSIDEONLY
	+BOUNCEONWALLS 
	+BOUNCEONACTORS
	+BOUNCEONFLOORS 
	+BOUNCEONCEILINGS 
	BounceCount 2
	BounceFactor 0.75
	scale 0.20
	Gravity 0.5
	Radius 3
	Height 4
	BounceSound "DeusEx/GibBounce"
	Var Int User_Angle;
	States
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_Jump(128,2)
		TNT1 A 0 A_SetUserVar(User_Angle,Random(6,20))
		Goto Decide
		TNT1 A 0 A_SetUserVar(User_Angle,Random(-20,-6))
		Goto Decide
	Decide:
		DX00 A 0 A_Jump(64,"Flying")
		Loop
	Flying:
		"----" AAAA 2 A_SpawnItemEx("DeusEx_BloodDrop",FRandom(0,8),0,FRandom(-8,8),velx,vely,velz-0.33,Random(1,360), SXF_TRANSFERTRANSLATION|SXF_ABSOLUTEVELOCITY)
		//"----" A 0 A_SetPitch(Pitch+User_Angle)
		"----" A 1 A_SetAngle(Angle+User_Angle)
		Loop
	Death:
		"----" A 500		
		"----" A 0 A_JumpIf(ACS_NamedExecuteWithResult("SamsaraDecorate",31) == 1, "Death")
		"----" A 35 A_FadeOut(0.5)
		Wait
	}
}

