Actor DeusEx_Rockchip 
{
	PROJECTILE
	-NOGRAVITY
	+NOBLOCKMAP
	+MOVEWITHSECTOR
	+THRUACTORS
	+NOTELEPORT
	-SOLID
	-TELESTOMP
	+BOUNCEAUTOOFF
	+CLIENTSIDEONLY
	+BOUNCEONWALLS 
	+BOUNCEONFLOORS 
	+BOUNCEONCEILINGS 
	BounceCount 2
	BounceFactor 0.75
	Gravity 0.66
	Scale 0.15
	Radius 3
	Height 4
	Speed 6
	BounceSound "DeusEx/HitStone"
	Var Int User_Angle;
	States
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_Jump(128,2)
		TNT1 A 0 A_SetUserVar(User_Angle,Random(6,20))
		Goto Decide
		TNT1 A 0 A_SetUserVar(User_Angle,Random(-20,-6))
		Goto Decide
	Decide:
		DX00 A 0 A_Jump(256, "Spawn1", "Spawn2", "Spawn3")
		Goto Spawn1
	Spawn1:
		DX00 A 1 A_Jump(64,"Flying")
		Loop
	Spawn2:
		DX01 A 1 A_Jump(64,"Flying")
		Loop	
	Spawn3:
		DX02 A 1 A_Jump(64,"Flying")
		Loop		
	Flying:
		"----" A 0 A_SetPitch(Pitch+User_Angle)
		"----" A 1 A_SetAngle(Angle+User_Angle)
		Loop
	Death:
		"----" A 500		
		"----" A 35 A_FadeOut(0.5)
		Wait
	}
}

Actor DeusEx_ShellCasing1
{
    Speed 2
    Mass 8
    Radius 2
    Height 2
	Scale 0.6
	+MISSILE
    +CLIENTSIDEONLY
    +THRUACTORS
	+DROPOFF
	+NOTELEPORT
	+FLOORCLIP
	-SOLID
    States
    {
    Spawn:
		DX00 A 0 A_SetPitch(Random(-40,25))
		DX00 A 0 A_SetAngle(Random(-50,50))
		Goto SpawnLoop
	SpawnLoop:	
		DX00 A 0 A_SetPitch(pitch-15)
		DX00 A 0 A_SetAngle(angle-15)
		DX00 A 1
        loop
    Death:
		DX00 A 0 A_JumpIf(waterlevel != 0,"Done")
		DX00 A 0 A_PlaySound("DeusEx/ShellCasing",CHAN_BODY)
		DX00 A 0 ThrustThingZ(0,4,0,0)
		goto Done
	Done:
		DX00 A 500
		DX00 A 1 A_FadeOut
		Goto Done+1
		}
}	

